[resource]
script = ExtResource( 1 )
syntax_version = 2
raw_text = "~ first_visit

if blue_joined
	The entrance to a large hall is to the north. To the west are two smaller corridors and to the east is one corridors.
	{{GREEN}}: Classes are divided into the three magical aspects {{YELLOW_ASPECT}}, {{GREEN_ASPECT}} and {{RED_ASPECT}}. The {{GREAT_HALL}} in front is big enough to fit all students in one go.
	{{BLUE}} nods.
else
	The entrance to a large hall is to the north. To the west are two smaller corridors and to the east is one corridors.
- Go through the corridor to the west => yellow_hallway
- Go through the corridor to the north-west => green_hallway
- Enter the {{GREAT_HALL}} in to the north => great_hall
- Go through the corridor to the north-east [if is_in_blue_space()]  => blue_hallway
- Go through the corridor to the east => red_hallway
- Go through the hallway to the south => main_entrance

~ visit

if in_blue_space
	if has_ouroboros
		{{GREEN}} is in the main hallway. A blue mist covers the view.
	else
		The shadowy figure seemingly went through the wall to the north-east. While {{GREEN}} wonders how, the blue mist disperses from that wall, revealing a new pathway.
		- Go through the corridor to the north-east  => blue_hallway
else
	{{GREEN}} is in the main hallway.
- Go through the corridor to the west => yellow_hallway
- Go through the corridor to the north-west => green_hallway
- Enter the {{GREAT_HALL}} in front => great_hall
- Go through the corridor to the north-east [if is_in_blue_space()]  => blue_hallway
- Go through the corridor to the east => red_hallway
- Go through the hallway to the south => main_entrance

~ execute_action

No problems here.
set next = \"main_hallway\"

~ yellow_hallway

if in_blue_space
	{{GREEN}} attempts to enter the corridor to the west. The blue mist thickens and when it clears, the path ends where {{GREEN}} started from!
	set next = \"main_hallway\"
else
	{{GREEN}} enters the corridor to the west.
	set next = \"yellow_corridor_end\"

~ green_hallway

if in_blue_space
	{{GREEN}} attempts to enter the corridor to the north-west. The blue mist thickens and when it clears, the path ends where {{GREEN}} started from!
	set next = \"main_hallway\"
else
	{{GREEN}} enters the corridor to the north-west.
	set next = \"green_corridor_end\"

~ great_hall

if in_blue_space
	{{GREEN}} attempts to enter the {{GREAT_HALL}}. The blue mist thickens and when it clears, the path ends where {{GREEN}} started from!
	set next = \"main_hallway\"
else
	{{GREEN}} enters the {{GREAT_HALL}}.
	set next = \"great_hall\"

~ blue_hallway

{{GREEN}} enters the corridor to the north-east.
set next = \"blue_corridor_end\"

~ red_hallway

if in_blue_space
	{{GREEN}} attempts to enter the corridor to the east. The blue mist thickens and when it clears, the path ends where {{GREEN}} started from!
	set next = \"main_hallway\"
else
	{{GREEN}} enters the corridor to the east.
	set next = \"red_corridor_end\"

~ main_entrance

{{GREEN}} walks through the hallway to the south.
set next = \"main_entrance\""
